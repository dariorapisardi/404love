{
  "name": "@404love/next",
  "version": "0.2.3",
  "description": "Next.js helpers for 404found.love",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
